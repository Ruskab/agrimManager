@startuml

!include configuration/skinParams.puml
!include configuration/definitions.puml

actor :User: as actor

view MenuView as b1
view DashboardView as b2
controller openDashboardController as c2
model InterventionsEntity as e3
model InterventionEntity as e4
model VehicleEntity as e5
model SessionEntity as e6

actor --> b1
